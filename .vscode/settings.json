{
    "python.analysis.include": [
        "self-correction/**"
    ],
    "python.analysis.exclude": [
        "**",
        "!self-correction/**"
    ],
    "python.analysis.autoSearchPaths": false,
    "python.analysis.extraPaths": [
        "./self-correction"
    ],
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.ruff_cache/**": true,
        "**/runs/**": true,
        "**/__pycache__/**": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*.pyd": true,
        "**/*.so": true,
        "**/*.egg": true,
        "**/*.egg-info/**": true,
        "**/*.dist-info/**": true,
        "**/*.whl": true,
        "**/*.tar.gz": true,
        "**/*.zip": true,
        "analyze_results.py": true,
        "get_plot.py": true,
        "list_printer.py": true,
        "registry.json": true,
        "improvements.txt": true,
        "model_performance_comparison.*": true
    },
    "python.defaultInterpreterPath": "./self-correction",
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.typeCheckingMode": "off"
} 